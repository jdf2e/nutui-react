    "@tarojs/vite-runner": "4.0.0-beta.138",
    "@tarojs/webpack5-runner": "4.0.0-beta.138",
    "@types/react": "^18.2.57",
    "@types/react-dom": "^18.2.19",
    "@types/react-router-dom": "^5.3.3",
    "@types/react-syntax-highlighter": "^15.5.11",
