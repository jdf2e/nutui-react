    "mobx": "^6.12.0",
    "mobx-react-lite": "^4.0.5",
    "postcss": "^8.4.35",
    "postcss-css-variables": "^0.19.0",
    "postcss-import": "^16.0.1",
    "postcss-modules": "^6.0.0",
    "postcss-rtlcss": "^5.1.0",
    "postcss-scss": "^4.0.9",
    "prettier": "^3.2.5",
    "react": "^18.2.0",
    "react-color": "^2.19.3",
