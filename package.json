    "react-test-renderer": "^18.2.0",
    "reactcss": "^1.2.3",
    "remark-directive": "^2.0.1",
    "remark-gfm": "^3.0.1",
    "rollup": "^3.18.0",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-dts": "^5.3.0",
    "sass": "^1.45.0",
    "shelljs": "^0.8.4",
    "ts-jest": "^27.1.4",
    "ts-node": "^10.7.0",
    "typescript": "^4.8.2",
    "unist-util-visit": "^4.1.0",
    "vinyl-fs": "^3.0.3",
